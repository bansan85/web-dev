{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "web-demangler": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "polyfills": [],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputPath": "dist/web-demangler",
              "outputHashing": "all",
              "tsConfig": "tsconfig.app.json",
              "index": "src/index.html",
              "browser": "src/main.ts",
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets",
                  "output": "/"
                },
                {
                  "glob": "**/*",
                  "input": "public"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "outputPath": "dist/web-demangler-dev",
              "sourceMap": true,
              "tsConfig": "tsconfig.app.json",
              "index": "src/index.html",
              "browser": "src/main.ts",
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets",
                  "output": "/"
                },
                {
                  "glob": "**/*",
                  "input": "public"
                }
              ]
            },
            "naming-style": {
              "outputPath": "dist/naming-style",
              "outputHashing": "all",
              "tsConfig": "tsconfig.app.naming-style.json",
              "index": {
                "input": "src/index-naming-style.html",
                "output": "index.html"
              },
              "browser": "src/main-naming-style.ts",
              "fileReplacements": [
                {
                  "replace": "src/app/app.routes.ts",
                  "with": "src/app/app.routes.naming-style.ts"
                }
              ],
              "assets": [
                {
                  "glob": "**/*",
                  "input": "public"
                }
              ]
            },
            "naming-style-dev": {
              "optimization": false,
              "extractLicenses": false,
              "outputPath": "dist/naming-style-dev",
              "sourceMap": true,
              "tsConfig": "tsconfig.app.naming-style.json",
              "index": {
                "input": "src/index-naming-style.html",
                "output": "index.html"
              },
              "browser": "src/main-naming-style.ts",
              "fileReplacements": [
                {
                  "replace": "src/app/app.routes.ts",
                  "with": "src/app/app.routes.naming-style.ts"
                }
              ],
              "assets": [
                {
                  "glob": "**/*",
                  "input": "public"
                }
              ]
            },
            "clang-format": {
              "outputPath": "dist/clang-format",
              "outputHashing": "all",
              "tsConfig": "tsconfig.app.clang-format.json",
              "index": {
                "input": "src/index-clang-format.html",
                "output": "index.html"
              },
              "browser": "src/main-clang-format.ts",
              "fileReplacements": [
                {
                  "replace": "src/app/app.routes.ts",
                  "with": "src/app/app.routes.clang-format.ts"
                }
              ],
              "assets": [
                {
                  "glob": "web_formatter.*",
                  "input": "src/assets",
                  "output": "/"
                },
                {
                  "glob": "**/*",
                  "input": "public"
                }
              ]
            },
            "clang-format-dev": {
              "optimization": false,
              "extractLicenses": false,
              "outputPath": "dist/clang-format-dev",
              "sourceMap": true,
              "tsConfig": "tsconfig.app.clang-format.json",
              "index": {
                "input": "src/index-clang-format.html",
                "output": "index.html"
              },
              "browser": "src/main-clang-format.ts",
              "fileReplacements": [
                {
                  "replace": "src/app/app.routes.ts",
                  "with": "src/app/app.routes.clang-format.ts"
                }
              ],
              "assets": [
                {
                  "glob": "web_formatter.*",
                  "input": "src/assets",
                  "output": "/"
                },
                {
                  "glob": "**/*",
                  "input": "public"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "naming-style": {
              "buildTarget": "web-demangler:build:naming-style"
            },
            "naming-style-dev": {
              "buildTarget": "web-demangler:build:naming-style-dev"
            },
            "clang-format": {
              "buildTarget": "web-demangler:build:clang-format"
            },
            "clang-format-dev": {
              "buildTarget": "web-demangler:build:clang-format-dev"
            },
            "production": {
              "buildTarget": "web-demangler:build:production"
            },
            "development": {
              "buildTarget": "web-demangler:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "runnerConfig": "vitest.config.ts"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/!(*.d).ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}